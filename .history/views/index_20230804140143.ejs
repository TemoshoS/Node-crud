<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>nodejs course</title>
</head>

<body>
    <%- include('partials/nav.ejs')%>


        <div class="page-header">
            <h2 class="job-header">
                all jobs
            </h2>

        </div>

        <div class="main-jobs-container">
            <% if(responseArray.length > 0){%>
                <%   responseArray.forEach((results)=>{%>

                    <div class="job-card">

                        <div class="job-details">
                            <div class="created-details">
                                <div>
                                    <img src="https://www.tutorialspoint.com/javafx/images/javafx-mini-logo.jpg" alt="profile" class="profile">
                                </div>
                                <div class="date-posted">
                                    <p>Admin</p>
                                    <p>03 June 2023</p>
                                </div>
                            </div>
                        </div>
                        <h2><%=results.title%></h2>
                    <p class="text"><%=results.description%></p>
        
        
                    </div>


              <%  })  %>

              <% } else {%>
                
                <h1>No jobs yet</h1>
              <%}%>
            
            
        </div>
        <%- include('partials/nav.ejs')%>
</body>

</html>